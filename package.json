{
  "name": "staft",
  "version": "1.0.0",
  "description": "A Staffing Organization's Operations Automation Web Application",
  "main": "server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "server": "nodemon src/server.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "check-types": "tsc --noEmit",
    "prettier": "prettier 'src/**/*.{js,ts}'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --check",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build -- prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Helping-Hand-LLC/staft.git"
  },
  "keywords": [],
  "author": "Skye Brown",
  "license": "",
  "bugs": {
    "url": "https://github.com/Helping-Hand-LLC/staft/issues"
  },
  "homepage": "https://github.com/Helping-Hand-LLC/staft#readme",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.6.1",
    "@types/socket.io": "^2.1.10",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "moment": "^2.25.3",
    "mongoose": "^5.9.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^6.8.1",
    "@types/express": "^4.17.6",
    "@types/googlemaps": "^3.39.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.152",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^14.0.5",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/prettier": "^2.0.1",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3"
  }
}
